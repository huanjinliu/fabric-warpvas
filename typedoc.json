{
  "entryPoints": ["src/index.ts", "src/modes/index.ts"],
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "theme": "markdown",
  "readme": "none",
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "hideGenerator": true,
  "disableSources": true,
  "hideBreadcrumbs": true,
  "skipErrorChecking": true
}
